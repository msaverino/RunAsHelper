<?xml version="1.0" encoding="utf-8" ?>
<root>

	<application name="Registry Editor">
		<path>C:\Windows\regedit.exe</path>
	</application>
	<application name="Edge">
		<path>C:\Program Files (x86)\Microsoft\Edge\Application\msedge.exe</path>
	</application>
	<application name="PowerShell - Pass C:\Utils\This.ps1">
		<path>C:\Windows\System32\WindowsPowerShell\v1.0\powershell.exe</path>
		<arguments>-file C:\Utils\This.ps1</arguments>
	</application>
	<application name="PowerShell - Custom">
		<path>C:\Windows\System32\WindowsPowerShell\v1.0\powershell.exe</path>
		<arguments custom="true"></arguments>
	</application>
	<application name="cmd">
		<path>C:\Windows\system32\cmd.exe</path>
	</application>
	<application name="PowerShell Pre-Defined Arguments">
		<path>C:\Windows\System32\WindowsPowerShell\v1.0\powershell.exe</path>
		<arguments>Write-Host "Hello";Start-Sleep 5</arguments>
	</application>
	<application name="PowerShell Pre-Defined Arguments with the ability to replace string ** Must use {Replace}">
		<path>C:\Windows\System32\WindowsPowerShell\v1.0\powershell.exe</path>
		<arguments>Write-Host "{Replace}";Start-Sleep 5</arguments>
		<PromptForArguments>true</PromptForArguments>
	</application>
	<application name="PowerShell Prompt the user to create arguments.">
		<path>C:\Windows\System32\WindowsPowerShell\v1.0\powershell.exe</path>
		<arguments></arguments>
		<PromptForArguments>true</PromptForArguments>
	</application>
	<application name="PowerShell Append the parameteres to the end">
		<path>C:\Windows\System32\WindowsPowerShell\v1.0\powershell.exe</path>
		<arguments>Write-Host "Hello";</arguments>
		<PromptForArguments>true</PromptForArguments>
	</application>
</root>